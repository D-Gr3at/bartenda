<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tribesquare.LayoutController">
   <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="550.0"
         prefWidth="1000.0">
      <TableView fx:id="tableView" prefHeight="550.0" prefWidth="800.0">
         <columns>
            <TableColumn fx:id="tableColumn" prefWidth="186.0" text="Table"/>
            <TableColumn fx:id="floorColumn" minWidth="0.0" prefWidth="204.0" text="Floor"/>
            <TableColumn fx:id="assignedColumn" prefWidth="202.0" text="Assigned To"/>
            <TableColumn fx:id="txColumn" prefWidth="207.0" text="Tx Number"/>
         </columns>
      </TableView>
      <VBox prefHeight="550.0" prefWidth="200.0">
         <AnchorPane prefHeight="550.0" prefWidth="200.0">
            <Button fx:id="createLayout" layoutX="25.0" layoutY="61.0" mnemonicParsing="false"
                    onAction="#createNewLayout" prefHeight="40.0" prefWidth="150.0" text="New"
                    textAlignment="CENTER"/>
            <Button fx:id="updateLayout" layoutX="25.0" layoutY="120.0" mnemonicParsing="false"
                    onAction="#updateLayout" prefHeight="40.0" prefWidth="150.0" text="Update"/>
            <Button fx:id="delete" layoutX="25.0" layoutY="181.0" mnemonicParsing="false"
                    onAction="#deleteLayout" prefHeight="40.0" prefWidth="150.0" text="Delete"/>
         </AnchorPane>
      </VBox>
   </HBox>
</AnchorPane>
